{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-implicit-dependencies": false,
    "interface-name": [false],
    "object-literal-sort-keys": false,
    "no-object-literal-type-assertion": false,
    "no-submodule-imports": false,
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"
    ]
  }
}
